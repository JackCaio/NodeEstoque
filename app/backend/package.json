    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
