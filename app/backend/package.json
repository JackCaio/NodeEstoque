{
  "name": "estoque-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "npx -y tsc",
    "predev": "npm run build && npm run db:reset",
    "dev": "ts-node-dev src/server.ts",
    "predb:reset": "npm run build",
    "db:reset": "npx sequelize db:drop && npx sequelize db:create",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./build/server.js",
    "lint": "eslint ./src --ext .ts",
    "test": "npx mocha -r ts-node/register ./src/tests/*.test.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "test:coverage:json": "nyc -r=json-summary npm run test"
  },
